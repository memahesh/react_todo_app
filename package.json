  "license": "MIT",
  "devDependencies": {},
  "dependencies": {
    "axios": "^0.19.0",
    "concurrently": "^5.0.0",
    "cors": "^2.8.5",
    "express": "^4.16.2",
    "nodemon": "^2.0.1"
  }
}
